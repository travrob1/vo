<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>Home | Nova</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">

        <link rel="stylesheet" href="bootstrap/dist/css/bootstrap.css">
        <link rel="stylesheet" href="font-awesome/css/font-awesome.min.css">
        <link rel="stylesheet" href="css/sl-slide.css">
        <link rel="stylesheet" href="ng-img-crop/compile/unminified/ng-img-crop.css">
        <link rel="stylesheet" href="ui-select/dist/select.css">
        <link rel="stylesheet" href="css/select2theme.css">
        <link rel="stylesheet" href="css/main.css">



        <script src="modernizr/modernizr-2.6.2-respond-1.1.0.min.js"></script>

        <!-- Le fav and touch icons -->
        <link rel="shortcut icon" href="images/ico/favicon.ico?v=3">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="images/ico/apple-touch-icon-144-precomposed.png?v=2">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="images/ico/apple-touch-icon-114-precomposed.png?v=2">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="images/ico/apple-touch-icon-72-precomposed.png?v=2">
        <link rel="apple-touch-icon-precomposed" href="images/ico/apple-touch-icon-57-precomposed.png?v=2">
        
        <script src="jquery/dist/jquery.min.js"></script>
        <script src="angular/angular.js"></script>
        <script src="angular-ui-router/release/angular-ui-router.js"></script>
        <script src="angular-ui-validate/dist/validate.js"></script>
        <script src="angular-resource/angular-resource.min.js"></script>
        <script src="angular-sanitize/angular-sanitize.js"></script>
        <script src="ng-file-upload/dist/ng-file-upload-all.min.js"></script>
        <script src="bootstrap/dist/js/bootstrap.js"></script>
        <script src="ng-img-crop/compile/unminified/ng-img-crop.js"></script>

        
        <script src="ui-select/dist/select.js"></script>
        <script src="lodash/lodash.js"></script>
        <script src="owasp-password-strength-test/owasp-password-strength-test.js"></script>


        <script src="js/all.js"></script>
        <script type='text/javascript'>
        angular.module('app.config', []).constant('config', {{{config}}});
        </script>
        <!-- /Required javascript files for Slider -->
    </head>
    

    <body ng-app='app' ng-controller="globalCtrl" >
        
        <ng-include src="'js/header/header.html'"></ng-include>
        
        <div id='notification-container' style="background-color: #fff" ng-show="notification" >
            <div class="container" style="position: relative">
                <div id="notifications" class="alert alert-danger"  ng-bind="notification"></div>
                <span ng-click="notification = undefined">x</span>
            </div>
        </div>

        <div ui-view id="ng-view">
        </div>

        <ng-include src="'js/footer/footer.html'"></ng-include>
        
    </body>
</html>
